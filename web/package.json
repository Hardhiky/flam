{
  "name": "edge-detector-web-viewer",
  "version": "1.0.0",
  "description": "TypeScript web viewer for Android Edge Detection app",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "npm run build && node server.js",
    "start": "node server.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "edge-detection",
    "opencv",
    "android",
    "typescript",
    "image-processing"
  ],
  "author": "RnD Intern",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2"
  }
}
